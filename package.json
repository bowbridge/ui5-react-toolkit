{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main"
  },
  "peerDependencies": {
    "@hookform/resolvers": "^2.8.2",
    "@ui5/webcomponents": "^1.0.2",
    "@ui5/webcomponents-fiori": "^1.0.2",
    "@ui5/webcomponents-react": "^0.20.2",
    "react": ">=16",
    "react-hook-form": "^7.17.4",
    "yup": "^0.32.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@bowbridge/ui5-react-toolkit",
  "author": "Webteam - bowbridge Software GmbH",
  "description": "Utility components and Functions for UI5 Web Components for React.",
  "module": "dist/ui5-react-toolkit.esm.js",
  "size-limit": [
    {
      "path": "dist/ui5-react-toolkit.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ui5-react-toolkit.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@hookform/resolvers": "^2.8.2",
    "@size-limit/preset-small-lib": "^6.0.3",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addons": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@ui5/webcomponents": "^1.0.2",
    "@ui5/webcomponents-fiori": "^1.0.2",
    "@ui5/webcomponents-react": "^0.20.2",
    "babel-loader": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.4",
    "react-is": "^17.0.2",
    "semantic-release": "^18.0.0",
    "size-limit": "^6.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "yup": "^0.32.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bowbridge/ui5-react-toolkit.git"
  }
}
