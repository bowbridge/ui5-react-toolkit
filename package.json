{
  "version": "0.0.1-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main"
  },
  "peerDependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@ui5/webcomponents": "^1.22.0",
    "@ui5/webcomponents-fiori": "^1.22.0",
    "@ui5/webcomponents-react": "^1.25.0",
    "react": ">=18",
    "react-hook-form": "^7.50.1",
    "yup": "^1.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@bowbridge/ui5-react-toolkit",
  "author": "Webteam - bowbridge Software GmbH",
  "description": "Utility components and Functions for UI5 Web Components for React.",
  "module": "dist/ui5-react-toolkit.esm.js",
  "size-limit": [
    {
      "path": "dist/ui5-react-toolkit.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ui5-react-toolkit.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@hookform/resolvers": "^3.3.4",
    "@size-limit/preset-small-lib": "^11.0.2",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addons": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@ui5/webcomponents": "^1.22.0",
    "@ui5/webcomponents-fiori": "^1.22.0",
    "@ui5/webcomponents-react": "^1.25.0",
    "babel-loader": "^9.1.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-is": "^18.2.0",
    "semantic-release": "^20.1.3",
    "size-limit": "^11.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "yup": "^1.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bowbridge/ui5-react-toolkit.git"
  }
